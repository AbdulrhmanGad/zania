<odoo>
    <data noupdate="1">
        <record id="owner_invoice_seq" model="ir.sequence">
            <field name="name">owner.invoice</field>
            <field name="code">owner.invoice</field>
            <field name="prefix">OWR/INV/</field>
            <field name="padding">5</field>
        </record>

        <record id="subcontractor_invoice_seq" model="ir.sequence">
            <field name="name">subcontractor.invoice</field>
            <field name="code">sub.invoice</field>
            <field name="prefix">SUB/INV/</field>
            <field name="padding">5</field>
        </record>
        <record id="owner_contract_seq" model="ir.sequence">
            <field name="name">owner.contract</field>
            <field name="code">owner.contract</field>
            <field name="prefix">OWR/CON/</field>
            <field name="padding">5</field>
        </record>
        <record id="sub_contract_seq" model="ir.sequence">
            <field name="name">sub.contract</field>
            <field name="code">sub.contract</field>
            <field name="prefix">SUB/CON/</field>
            <field name="padding">5</field>
        </record>

        <record id="financial_offer_seq" model="ir.sequence">
            <field name="name">financial.offer</field>
            <field name="code">financial.offer</field>
            <field name="prefix">Fin/Offer</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>